{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aksdatafact"
		},
		"ls_backenddata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_backenddata'"
		},
		"ls_bronzedata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_bronzedata'"
		},
		"ls_gitbackenddata_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'ls_gitbackenddata'"
		},
		"ls_bronzedata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bronzeraw1.dfs.core.windows.net/"
		},
		"ls_gitbackenddata_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Akshith-98"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aksdatafact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_backenddata_connectionString": {
						"value": "[parameters('ls_backenddata_connectionString')]"
					},
					"ls_bronzedata_accountKey": {
						"value": "[parameters('ls_bronzedata_accountKey')]"
					},
					"ls_gitbackenddata_credential": {
						"value": "[parameters('ls_gitbackenddata_credential')]"
					},
					"ls_bronzedata_properties_typeProperties_url": {
						"value": "[parameters('ls_bronzedata_properties_typeProperties_url')]"
					},
					"ls_gitbackenddata_properties_typeProperties_username": {
						"value": "[parameters('ls_gitbackenddata_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}